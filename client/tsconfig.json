{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "removeComments": false,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "outDir": "lib",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "baseUrl": ".",
    "rootDir": ".",
    "jsx": "preserve",
    "jsxFactory": "Taro.createElement",
    "allowJs": true,
    "resolveJsonModule": true,
    "typeRoots": [
      "node_modules/@types",
      "global.d.ts"
    ],
    "paths": {
      "@/ts-types/*": [
        "src/ts-types/*",
      ],
      "@/ts-types": [
        "src/ts-types/*",
      ],
      "@/components/*": [
        "src/components/*",
      ],
      "@/components": [
        "src/components",
      ],
      "@/utils/*": [
        "src/utils/*",
      ],
      "@/utils": [
        "src/utils",
      ],
      "@/config/*": [
        "src/config/*",
      ],
      "@config": [
        "src/config",
      ],
      "@/services/*": [
        "src/services/*",
      ],
      "@/services": [
        "src/services",
      ],
      "@/containers/*": [
        "src/containers/*",
      ],
      "@/containers": [
        "src/containers",
      ],
      "@/hooks/*": [
        "src/hooks/*",
      ],
      "@/hooks": [
        "src/hooks",
      ],
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "compileOnSave": false
}